{
  "name": "@jupyterlab/example-federated",
  "version": "2.4.0-alpha.7",
  "private": true,
  "scripts": {
    "build": "jlpm run build:core && jlpm run build:middle && jlpm run build:md && jlpm run build:phosphor",
    "build:core": "cd core_package && jlpm run build",
    "build:md": "cd md_package && jlpm run build",
    "build:middle": "cd middle_package && jlpm run build",
    "build:phosphor": "cd phosphor_package && jlpm run build",
    "install:all": "jlpm run install:core && jlpm run install:middle && jlpm run install:md && jlpm run install:phosphor",
    "install:core": "cd core_package && jlpm install",
    "install:md": "cd md_package && jlpm install",
    "install:middle": "cd middle_package && jlpm install",
    "install:phosphor": "cd phosphor_package && jlpm install"
  }
}
