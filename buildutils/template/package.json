{
  "name": "@jupyterlab/template",
  "version": "3.3.0-alpha.7",
  "description": "JupyterLab - Package Template",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "scripts": {
    "build": "$npm_execpath run tsc -b",
    "build:all": "$npm_execpath run build",
    "build:test": "$npm_execpath run tsc --build tsconfig.test.json",
    "clean": "$npm_execpath run rimraf lib && $npm_execpath run rimraf tsconfig.tsbuildinfo",
    "test": "$npm_execpath run jest",
    "test:cov": "$npm_execpath run jest --collect-coverage",
    "test:debug": "$npm_execpath run node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug:watch": "$npm_execpath run node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "watch": "$npm_execpath run tsc -b --watch"
  },
  "devDependencies": {
    "@jupyterlab/testutils": "^3.3.0-alpha.7",
    "@types/jest": "^26.0.10",
    "jest": "^26.4.2",
    "rimraf": "~3.0.0",
    "ts-jest": "^26.3.0",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
