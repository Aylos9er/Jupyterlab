{
  "type": "object",
  "title": "Metadata Form",
  "description": "Settings of the metadata form extension.",
  "jupyter.lab.metadataforms": [
    {
      "label": "NEW_extension",
      "metadataKeys": [
        {
          "metadataKey": ["my-extension", "active"],
          "properties": {
            "title": "is-active",
            "description": "Active status",
            "type": "boolean"
          }
        },
        {
          "metadataKey": ["my-extension", "name"],
          "properties": {
            "title": "name",
            "description": "Unique name",
            "type": "string"
          }
        },
        {
          "metadataKey": ["my-extension", "sub", "other_sub", "test"],
          "properties": {
            "title": "subsub",
            "description": "test on subsub",
            "type": "string"
          }
        },
        {
          "metadataKey": ["root-value"],
          "properties": {
            "title": "root",
            "description": "test on root",
            "type": "integer"
          }
        }
      ],
      "mainKey": "my-extension",
      "_origin": "@jupyterlab/metadataform-extension:metadataforms"
    }
  ],
  "jupyter.lab.transform": true,
  "additionalProperties": false,
  "properties": {
    "metadataforms": {
      "items": {
        "$ref": "#/definitions/metadataForm"
      },
      "type": "array",
      "default": []
    }
  },
  "definitions": {
    "metadataForm": {
      "type": "object",
      "properties": {
        "mainKey": {
          "type": "string",
          "description": "The main metadata key"
        },
        "label": {
          "type": "string",
          "description": "The section label"
        },
        "rank": {
          "type": "integer",
          "description": "The rank of the section in the right panel"
        },
        "metadataKeys": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/userProperties"
          }
        }
      },
      "required": ["label", "metadataKeys"]
    },
    "userProperties": {
      "properties": {
        "properties": {
          "type": "object",
          "description": "The property set up by extension",
          "properties": {
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "type": {
              "type": "string"
            }
          }
        },
        "metadataKey": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "type": "object",
      "required": ["metadataKey", "properties"]
    }
  }
}
