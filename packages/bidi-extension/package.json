{
  "name": "@jupyterlab/bidi-extension",
  "version": "0.1.0",
  "description": "add btd and stt support in jupyterLab",
  "author": "bdl",
  "main": "lib/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
